#!/bin/bash
curl https://cdn.star.nesdis.noaa.gov/GOES16/ABI/FD/GEOCOLOR/1808x1808.jpg > /home/afrl/Downloads/earth.jpg
GIO_EXTRA_MODULES="/usr/lib/x86_64-linux-gnu/gio/modules/"; export $GIO_EXTRA_MODULES
gsettings set org.gnome.desktop.background picture-uri file:///home/afrl/Downloads/earth.jpg
/
